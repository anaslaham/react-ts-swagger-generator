{
  "name": "react-ts-swagger-generator",
  "version": "1.0.0",
  "description": "a cli that generates ts interface and a react context from a swagger json file",
  "main": "dist/app.js",
  "scripts": {
    "start": "webpack && node dist/bundle.js"
  },
  "keywords": [
    "cli",
    "code",
    "generator",
    "js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g"
    }
  },
  "author": "Anas Laham",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "axios": "^0.21.1",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "is-my-json-valid": "^2.20.5",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/swagger-schema-official": "^2.0.21",
    "handlebars-loader": "^1.7.1",
    "husky": "^4.3.8",
    "pretty-quick": "^3.1.0",
    "ts-loader": "^8.0.18",
    "tslint": "^5.20.1",
    "typescript": "^3.9.9",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
