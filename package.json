{
  "name": "react-ts-swagger-generator",
  "version": "1.0.0",
  "description": "a cli that generates ts interface and a react context from a swagger json file",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "code",
    "generator",
    "js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g"
    }
  },
  "author": "Anas Laham",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "handlebars": "^4.7.6",
    "is-my-json-valid": "^2.20.5",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/swagger-schema-official": "^2.0.21",
    "husky": "^4.3.6",
    "pretty-quick": "^3.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  }
}
